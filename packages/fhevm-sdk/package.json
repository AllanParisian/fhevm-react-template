{
  "name": "@fhevm-template/sdk",
  "version": "1.0.0",
  "description": "Universal SDK for building privacy-preserving dApps with Zama fhEVM - framework-agnostic, developer-friendly",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --module esnext --outDir dist",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fhevm",
    "zama",
    "encryption",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk",
    "wagmi-like",
    "framework-agnostic"
  ],
  "author": "Privacy Compliance Team",
  "license": "MIT",
  "peerDependencies": {
    "ethers": "^6.9.0"
  },
  "dependencies": {
    "fhevm": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fhevm-react-template"
  }
}
